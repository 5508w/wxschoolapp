.top{
  border-top: 5px #66CCFF solid;
  width: 100%;
  height: 80px;
  background-color: #66CCFF;display: flex;
  flex-direction: row;
}
.top image{
  border-radius: 50%;
  margin-left: 10px;
}
.nickName{
  color: #ffffff;
  font-size: 15px;
  position: absolute;
  left: 100px;
  margin-top:30px;
}
.tableviewcell {
	padding: 20rpx 40rpx;
	border-bottom: 1rpx solid #ddd;
	color: #666;
	background: white;
	display: flex;
	align-items: center;
}

/*icon*/
.tableviewcell image {
	width: 60rpx;
	height: 60rpx;
}

/*text*/
.tableviewcell text {
	font-size: 16px;
	margin-left: 10px;
}